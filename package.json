{
  "name": "unie",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx prisma migrate dev",
    "generate:client": "npx prisma generate",
    "studio": "npx prisma studio",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.development up -d",
    "docker:dev:build": "docker compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.development up -d --build",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml --env-file .env.production up -d",
    "docker:prod:build": "docker compose -f docker-compose.yml -f docker-compose.prod.yml --env-file .env.production up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:clean": "docker compose down -v --remove-orphans",
    "docker:studio": "docker compose exec nextjs npx prisma studio",
    "docker:studio:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec nextjs npx prisma studio"
  },
  "dependencies": {
    "@adobe/react-spectrum": "3.39.0",
    "@emotion/is-prop-valid": "1.3.1",
    "@prisma/client": "6.1.0",
    "bcryptjs": "2.4.3",
    "eslint-import-resolver-alias": "1.1.2",
    "jsonwebtoken": "9.0.2",
    "minio": "8.0.4",
    "nanoid": "5.1.0",
    "next": "15.1.6",
    "next-auth": "4.24.11",
    "pg": "8.13.1",
    "prisma": "6.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-select": "5.10.0",
    "styled-components": "6.1.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "autoprefixer": "10.4.20",
    "eslint": "9",
    "eslint-config-next": "15.1.3",
    "postcss": "8",
    "tailwindcss": "3.4.1"
  },
  "engines": {
    "node": ">=18"
  }
}
